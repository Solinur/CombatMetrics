<GuiXml xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://sir.insidi.at/or/schema/esoui.xsd">
    <Controls>
        <TopLevelControl name="CombatMetricsReport" clampedToScreen="true" mouseEnabled="true" movable="true" hidden="true">
            <Dimensions x="1044" y="770" />
            <Controls>
                <Backdrop name="$(parent)BG" inherits="CombatMetrics_BackgroundFramed_Solid" edgeColor="FF000000" centerColor="EE000000">
                    <AnchorFill />
                </Backdrop>

                <Backdrop name="$(parent)ResizeFrame" inherits="CombatMetrics_BackgroundFramed_Solid" edgeColor="00000000"
                    centerColor="00000000" mouseEnabled="true" resizeHandleSize="10" layer="CONTROLS">
                    <AnchorFill />
                    <DimensionConstraints minX="500" maxX="3000" minY="385" maxY="2310" />
                    <OnRectChanged>CombatMetrics.internal.NewSize(self, newLeft, newTop, newRight, newBottom, oldLeft, oldTop, oldRight, oldBottom)</OnRectChanged>
                    <OnResizeStart>CombatMetrics.internal.Resizing(self, true)</OnResizeStart>
                    <OnResizeStop>CombatMetrics.internal.Resizing(self, false)</OnResizeStop>
                </Backdrop>

                <Control name="$(parent)_Title" inherits="CombatMetrics_TitleBar">
                    <Anchor point="TOPLEFT" offsetX="4" offsetY="4" />
                    <Anchor point="TOPRIGHT" offsetX="-4" offsetY="4" />
                    <Dimensions y="48"/>
                    <OnInitialized>CombatMetrics.internal.InitializeTitlePanel(self)</OnInitialized>
                </Control>

                <Control name="$(parent)_Menu" layer="BACKGROUND" >
                    <Anchor point="TOPLEFT" relativePoint="BOTTOMLEFT" relativeTo="$(parent)_Title" offsetY="4" />
                    <Anchor point="BOTTOMLEFT" offsetY="-4" constrains="Y"/>
                    <Dimensions x="32"/>
                    <OnInitialized>CombatMetrics.internal.InitializeMenuPanel(self)</OnInitialized>
                </Control>

                <Control name="$(parent)_InfoRow" inherits="CombatMetrics_InfoRow">
                    <Anchor point="BOTTOMLEFT" relativePoint="BOTTOMRIGHT" relativeTo="$(parent)_Menu" offsetX="4" />
                    <Anchor point="BOTTOMRIGHT" offsetX="-4" offsetY="-4" constrains="X"/>
                    <Dimensions y="24"/>
                    <OnInitialized>CombatMetrics.internal.InitializeInfoRowPanel(self)</OnInitialized>
                </Control>

                <!-- <Control name="$(parent)_MainPanel" inherits="CombatMetrics_ControlFramed">
                    <Anchor point="TOPLEFT" relativePoint="TOPRIGHT" relativeTo="$(parent)_Menubar" offsetY="4" />
                    <Dimensions x="628" y="374" />
                </Control> -->

                <Control name="$(parent)_CombatStats" inherits="CombatMetrics_ControlFramed">
                    <Anchor point="TOPLEFT" relativePoint="TOPRIGHT" relativeTo="$(parent)_Menu" offsetX="4"/>
                    <Dimensions x="346" y="374" />
                    <OnInitialized>CombatMetrics.internal.InitializeCombatStatsPanel(self)</OnInitialized>
                </Control>
                
                <Control name="$(parent)_PlayerStats" inherits="CombatMetrics_PlaceHolder"> <!-- CombatMetrics_PlayerStatsPanel -->
                    <Anchor point="TOPLEFT" relativePoint="TOPRIGHT" relativeTo="$(parent)_CombatStats" offsetX="4"/>
                    <Dimensions x="278" y="374" />
                    <!-- <OnInitialized>CombatMetrics.internal.InitializePlayerStatsPanel(self)</OnInitialized> -->
                </Control>

                <Control name="$(parent)_BuffsPanel" inherits="CombatMetrics_BuffsPanel">
                    <Anchor point="TOPRIGHT" relativePoint="BOTTOMRIGHT" relativeTo="$(parent)_Title" offsetY="4" />
                    <Anchor point="BOTTOMLEFT" relativePoint="BOTTOMRIGHT" relativeTo="$(parent)_PlayerStats" offsetX="4" />
                    <OnInitialized>CombatMetrics.internal.InitializeBuffsPanel(self)</OnInitialized>
                </Control>

                <Control name="$(parent)_UnitsPanel" inherits="CombatMetrics_PlaceHolder"> <!-- CombatMetrics_UnitsPanel -->
                    <Anchor point="TOPLEFT" relativePoint="BOTTOMLEFT" relativeTo="$(parent)_CombatStats" offsetY="4" constrains="Y" />
                    <Anchor point="BOTTOMLEFT" relativePoint="TOPLEFT" relativeTo="$(parent)_InfoRow" offsetY="-4" />
                    <Dimensions x="360" />
                    <!-- <OnInitialized>CombatMetrics.internal.InitializeUnitsPanel(self)</OnInitialized> -->
                </Control>

                <Control name="$(parent)_AbilitiesPanel" inherits="CombatMetrics_PlaceHolder"> <!-- CombatMetrics_AbilitiesPanel -->
                    <Anchor point="TOPRIGHT" relativePoint="BOTTOMRIGHT" relativeTo="$(parent)_BuffsPanel" offsetY="4" />
                    <Anchor point="BOTTOMLEFT" relativePoint="BOTTOMRIGHT" relativeTo="$(parent)_UnitsPanel" offsetX="4" />
                    <!-- <OnInitialized>CombatMetrics.internal.InitializeAbilitiesPanel(self)</OnInitialized> -->
                </Control>

                <Control name="$(parent)_CombatLog" hidden="true" inherits="CombatMetrics_PlaceHolder"> <!-- CombatMetrics_CombatLog -->
                    <Anchor point="TOPLEFT" relativeTo="$(parent)_CombatStats"/>
                    <Anchor point="BOTTOMRIGHT" relativeTo="$(parent)_PlayerStats"/>
                    <!-- <OnInitialized>CombatMetrics.internal.InitializeCombatLogPanel(self)</OnInitialized> -->
                </Control>

                <Control name="$(parent)_Graph" hidden="true" inherits="CombatMetrics_PlaceHolder"> <!-- CombatMetrics_GraphPanel -->
                    <Anchor point="TOPLEFT" relativeTo="$(parent)_CombatStats"/>
                    <Anchor point="BOTTOMRIGHT" relativeTo="$(parent)_PlayerStats"/>
                    <!-- <OnInitialized>CombatMetrics.internal.InitializeGraphPanel(self)</OnInitialized> -->
                </Control>

                <Control name="$(parent)_SkillsPanel" hidden="true" inherits="CombatMetrics_PlaceHolder"> <!-- CombatMetrics_SkillsPanel -->
                    <Anchor point="TOPLEFT" relativeTo="$(parent)_CombatStats"/>
                    <Dimensions x="626" y="270" />
                    <!-- <OnInitialized>CombatMetrics.internal.InitializeSkillsPanel(self)</OnInitialized> -->
                </Control>

                <Control name="$(parent)_ScribedSkillsPanel" hidden="true" inherits="CombatMetrics_PlaceHolder"> <!-- resizeToFitDescendents="true" -->
                    <Anchor point="TOPLEFT" relativePoint="TOPRIGHT" relativeTo="$(parent)_SkillsPanel" offsetX="4" />
                    <Anchor point="TOPRIGHT" relativeTo="$(parent)_BuffsPanel" constrains="X"/>
                    <DimensionConstraints minY="0" />
                    <!-- <OnInitialized>CombatMetrics.internal.InitializeScribedSkillsPanel(self)</OnInitialized> -->
                </Control>

                <Control name="$(parent)_ChampionPointsPanel" hidden="true" inherits="CombatMetrics_PlaceHolder"> <!-- ZO_ScrollContainer -->
                    <Anchor point="TOPLEFT" relativePoint="BOTTOMLEFT" relativeTo="$(parent)_ScribedSkillsPanel" offsetY="4" />
                    <Anchor point="BOTTOMRIGHT" relativeTo="$(parent)_AbilitiesPanel" offsetY="-2" />
                    <!-- <OnInitialized>CombatMetrics.internal.InitializeChampionPointsPanel(self)</OnInitialized> -->
                </Control>

                <Control name="$(parent)_ConsumablesPanel" hidden="true" inherits="CombatMetrics_PlaceHolder"> <!-- CombatMetrics_ConsumablesPanel -->
                    <Anchor point="BOTTOMLEFT" relativeTo="$(parent)_UnitsPanel" offsetY="-4" />
                    <Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMLEFT" relativeTo="$(parent)_ChampionPointsPanel" offsetX="-4" constrains="X"/>
                    <Dimensions y="46" />
                    <!-- <OnInitialized>CombatMetrics.internal.InitializeConsumablesPanel(self)</OnInitialized> -->
                </Control>

                <Control name="$(parent)_EquipmentPanel" hidden="true" inherits="CombatMetrics_PlaceHolder"> <!-- CombatMetrics_EquipmentPanel -->
                    <Anchor point="TOPLEFT" relativePoint="BOTTOMLEFT" relativeTo="$(parent)_SkillsPanel" offsetY="4" />
                    <Anchor point="BOTTOMRIGHT" relativePoint="TOPRIGHT" relativeTo="$(parent)_ConsumablesPanel" offsetY="-4" />
                    <!-- <OnInitialized>CombatMetrics.internal.InitializeEquipmentPanel(self)</OnInitialized> -->
                </Control>

                <Control name="$(parent)_FightList" hidden="true" inherits="CombatMetrics_PlaceHolder"> <!-- CombatMetrics_FightListPanel -->
                    <Anchor point="TOPLEFT" relativeTo="$(parent)_CombatStats" />
                    <Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT" relativeTo="$(parent)_AbilitiesPanel" />
                    <!-- <OnInitialized>CombatMetrics.internal.InitializeFightListPanel(self)</OnInitialized> -->
                </Control>
            </Controls>
        </TopLevelControl>
    </Controls>
</GuiXml>