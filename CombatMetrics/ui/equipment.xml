                        offsetY="10" />
                </Control>

                <Control name="$(parent)EquipLine13" inherits="CombatMetrics_EquipLine">
                    <Anchor point="TOPLEFT" relativeTo="$(parent)EquipLine12" relativePoint="BOTTOMLEFT" offsetX="0"
                        offsetY="2" />
                    <Anchor point="TOPRIGHT" relativeTo="$(parent)EquipLine12" relativePoint="BOTTOMRIGHT" offsetX="0"
                        offsetY="2" />
                </Control>

                <Control name="$(parent)EquipLine14" inherits="CombatMetrics_EquipLine">
                    <Anchor point="TOPLEFT" relativeTo="$(parent)EquipLine13" relativePoint="BOTTOMLEFT" offsetX="0"
                        offsetY="2" />
                    <Anchor point="TOPRIGHT" relativeTo="$(parent)EquipLine13" relativePoint="BOTTOMRIGHT" offsetX="0"
                        offsetY="2" />
                </Control>
            </Controls>
        </Control>
    </Controls>
</GuiXml>